{
  "$schema": "http://json.schemastore.org/tsconfig",
  "compileOnSave": true,
  "compilerOptions": {
    "module": "none",
    "target": "es6",
    "skipLibCheck": true,
    "lib": [
      "dom",
      "scripthost",
      "es6",
      "es2017.intl"
    ],
    "types": [],
    "declaration": true,
    "sourceMap": true,
    "noEmitOnError": true,
    "suppressImplicitAnyIndexErrors": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictBindCallApply": true,
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "strictFunctionTypes": false,
    "strictPropertyInitialization": false,
    "alwaysStrict": false
  },
  "include": [
    "$(Beckhoff.TwinCAT.HMI.Framework).InstallPath/TcHmi.d.ts"
  ],
  "files": [
    "Scripts/typings/jqueryui/jqueryui.d.ts",
    "Scripts/typings/jquery/jquery.d.ts",
    "Widgets\\Progressbar/Progressbar.ts",
    "Widgets/Progressbar/Progressbar.ts",
    "Widgets\\Slider/Slider.ts",
    "Widgets/Slider/Slider.ts",
    "Interactions/Draggable.ts"
  ]
}